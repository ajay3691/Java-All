package DataTypes3;

 public class Test {

	public static void main(String[] args) {
		 byte b  = 100;
		 short s = 4343;
		 
		 int a  = 100;
		 int a1 = 53522;
		 int a2 = "Hllo"
		 System.out.println(b);	
		 System.out.println(s);	
		 System.out.println(a);	
		 System.out.println(a2);	

	}
}
 // byte Data type -: -128 to 127        1 byte
// short Data type -: -32768 to 32767    2 bytes
// Int  data type  -: -2147483648 to -   4 bytes 