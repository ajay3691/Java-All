package DataTypes3;

 public class Test {

	public static void main(String[] args) {
		 byte b  = 100;
		 short s = 4343;
		 
		 int a  = 100;
		 int a1 = 53522;
		 System.out.println(b);	
		 System.out.println(s);	
		 System.out.println(a);	
		 System.out.println(a1);	

	}
}
 // byte Data type -: -128 to 127
// short Data type -: -32768 to 32767